<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.youngtube.demo.mapper.VideoClickMapper">

    <select id="findLikeCount" resultType="java.lang.Integer">
      select count(*) from videoClick where videoId=#{videoId} and clickType=0
    </select>

    <select id="findDislikeCount" resultType="java.lang.Integer">
      select count(*) from videoClick where videoId=#{videoId} and clickType=1
    </select>

</mapper>